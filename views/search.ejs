<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>Movie Tracer</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

<link href="/css/search-style.css" rel="stylesheet" type="text/css" />
<script src="/js/jquery-1.4.2.min.js" type="text/javascript"></script>
<script src="/js/cufon-yui.js" type="text/javascript"></script>
<script src="/js/cufon-replace.js" type="text/javascript"></script>
<script src="/js/Gill_Sans_400.font.js" type="text/javascript"></script>
<script src="/js/script.js" type="text/javascript"></script>

<!-- Original content -->
<link rel="stylesheet" href="bootstrap-3.3.7-dist/css/bootstrap.min.css">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

</head>

<body id="page1">

<!-- START PAGE SOURCE -->
<div class="tail-top">
  <div class="tail-bottom">
    <div id="main">
      <div id="header">
        <div class="row-1">
          <div class="fleft"><i class="fa fa-film" aria-hidden="true"></i> Movie <span>Tracer</span></div>
        </div>
        <div class="row-2">
          <ul>
            <li><a href="#" class="active">Home</a></li>
            <li><a href="#trending">Trending</a></li>
            <li><a href="#discover">Discover</a></li>
            <li><a href="#cf">Contact Us</a></li>
            <li class="last"><a href="#letter">Subscribe</a></li>
          </ul>
        </div>
      </div>

      <div id="content">
        <div id="slogan">
          <div class="image png"></div>
          <div class="inside">
            <h2>Explore<span>The Cinema World</span></h2>
            <!-- <p>Lorem ipsum dolor consectetur adipisicing elit, sed do eiusmod tempor incididunt labore etolore magna aliqua enim minim veniam quis nostrud exercitation ullamco laboris.</p> -->
          </div>
        </div>
        <div class="box">
          <div class="border-right">
            <div class="border-left">
              <div id="discover" class="inner">
                <h3>Welcome to <b>Movie</b> <span>Tracer</span></h3>

                <!-- Original content -->
                <div class="search-content">
                  <h2>Discover Great Movies</h2>
                  <form action="/results" method="GET">
                    <div class="input-group">
                      <input type="text" placeholder="search term" name="search" class="form-control">
                      <span class="input-group-btn">
                        <button type="submit" class="btn btn-primary">Go!</button>
                      </span>
                    </div>  <!-- /input-group -->
                  </form>
                </div>

              </div>
            </div>
          </div>
        </div>

        <div class="row content" id="trending">
          <h3>Box <span>Office</span></h3>

          <% homeData["boxOffice"].forEach(function(value, index) { %>
            <div class="movie col-lg-4 col-sm-4 thumbnail">

              <div class="border-right">
                <div class="border-left">
                  <div class="title">
                    <form action="/movie" method="GET">
                      <button type="submit" name="title" value="<%= value["title"] %>" class="btn btn-link"><%= value["title"] %></button>
                    </form>
                    <!-- <%= value["title"] %> -->
                  </div>
                  <div class="gross">
                    <%= value["gross"] %>
                  </div>
                </div>
              </div>

            </div>
          <% }); %>
        </div>

        <div class="row content">
          <h3>Opening <span>This Week</span></h3>

          <% homeData["openingThisWeek"].forEach(function(value, index) { %>
            <div class="movie col-lg-4 col-sm-6 thumbnail">

              <div class="border-right">
                <div class="border-left">
                  <div class="title">
                    <form action="/movie" method="GET">
                      <button type="submit" name="title" value="<%= value["title"] %>" class="btn btn-link"><%= value["title"] %></button>
                    </form>
                    <!-- <%= value["title"] %> -->
                  </div>
                  <div class="date">
                    <%= value["date"] %>
                  </div>
                </div>
              </div>

            </div>
          <% }); %>
        </div>

        <div class="row content">
          <h3>Coming <span>Soon</span></h3>

          <% homeData["comingSoon"].forEach(function(value, index) { %>
            <div class="movie col-lg-4 col-sm-6 thumbnail">

              <div class="border-right">
                <div class="border-left">
                  <div class="title">
                    <form action="/movie" method="GET">
                      <button type="submit" name="title" value="<%= value["title"] %>" class="btn btn-link"><%= value["title"] %></button>
                    </form>
                    <!-- <%= value["title"] %> -->
                  </div>
                  <div class="date">
                    <%= value["date"] %>
                  </div>
                </div>
              </div>

            </div>
          <% }); %>
        </div>

        <!-- Contact Form -->
        <div id="cf" class="row content">
          <h3>Contact <span>Form</span></h3>
          <form id="contacts-form" action="#">
            <fieldset>
              <div class="field">
                <label>Your Name:</label>
                <input type="text" value=""/>
              </div>
              <div class="field">
                <label>Your E-mail:</label>
                <input type="text" value=""/>
              </div>
              <div class="field">
                <label>Your Website:</label>
                <input type="text" value=""/>
              </div>
              <div class="field">
                <label>Your Message:</label>
                <textarea cols="1" rows="1"></textarea>
              </div>
              <div class="wrapper"> <a href="#" class="link2"> <span> <span>Send Your Message</span> </span> </a> </div>
            </fieldset>
          </form>
        </div>

        <div id="letter" class="subscribe content row">
          <h3>Subscribe To Our <span>Newsletter</span></h3>
          <form action="/results" method="GET">
            <div class="input-group">
              <input type="text" placeholder="Enter your email" name="search" class="form-control">
              <span class="input-group-btn">
                <button type="submit" class="btn btn-danger">Subscribe</button>
              </span>
            </div>  <!-- /input-group -->
          </form>
        </div>
      </div>

    </div>
  </div>
</div>
<script type="text/javascript"> Cufon.now(); </script>
<!-- END PAGE SOURCE -->
</body>
</html>
